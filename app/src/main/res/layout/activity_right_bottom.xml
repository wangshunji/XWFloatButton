<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:fab="http://schemas.android.com/apk/res-auto"
                android:background="@color/background"
                android:layout_width="match_parent"
    android:id="@+id/rl"
                android:layout_height="match_parent">
<TextView
    android:id="@+id/clicktv"
    android:layout_marginTop="30dp"
    android:text="dddddddddd"
    android:textColor="@color/orange"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" />
    <View
        android:id="@+id/bg_view"
        android:visibility="gone"
        android:clickable="false"
        android:contextClickable="false"
        android:background="@color/blue_semi_transparent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <!--右边最底下那个组按钮-->
    <wang.shunji.xwfab.FloatingActionsMenu
            android:id="@+id/multiple_actions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            fab:fab_addButtonColorNormal="@color/white"
            fab:fab_addButtonColorPressed="@color/white_pressed"
            fab:fab_addButtonPlusIconColor="@color/half_black"
            fab:fab_labelStyle="@style/labels_style_orange"
            android:layout_marginBottom="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp">


        <!--右下角顶上那个-->
        <wang.shunji.xwfab.FloatingActionButton
                android:id="@+id/action_a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:fab_colorNormal="@color/white"
                fab:fab_colorPressed="@color/white_pressed"/>
        <!--右下角顶上第二个-->
        <wang.shunji.xwfab.FloatingActionButton
                android:id="@+id/action_b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:fab_colorNormal="@color/white"
                fab:fab_colorPressed="@color/white_pressed"/>

    </wang.shunji.xwfab.FloatingActionsMenu>


</RelativeLayout>
